{
  "name": "gulp-cssnext",
  "version": "0.4.0",
  "description": "Use tomorrow's CSS syntax, today. Via Gulp.",
  "keywords": [
    "css",
    "preprocessor",
    "postprocessor",
    "rework",
    "postcss",
    "autoprefixer",
    "gulpplugin"
  ],
  "author": "bloodyowl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/cssnext/gulp-cssnext.git"
  },
  "homepage": "http://cssnext.github.io",
  "bugs": {
    "url": "https://github.com/cssnext/gulp-cssnext/issues"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "index.js"
  ],
  "dependencies": {
    "cssnext": "^0.4.1",
    "gulp-util": "^3.0.0",
    "through2": "^0.6.1"
  },
  "devDependencies": {
    "jscs": "^1.5.9",
    "jshint": "^2.5.2",
    "jshint-stylish": "^0.4.0",
    "tap-colorize": "^1.2.0",
    "tape": "^2.14.0"
  },
  "scripts": {
    "jscs": "jscs *.js **/*.js",
    "jshint": "jshint . --exclude-path .gitignore --reporter node_modules/jshint-stylish/stylish.js",
    "lint": "npm run jscs && npm run jshint",
    "tape": "tape test/**/*.js | tap-colorize",
    "test": "npm run lint && npm run tape"
  }
}
